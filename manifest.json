{
  "manifest_version": 3,
  "name": "Betherscan",
  "description": "Improved Etherscan",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["utils/constants.js", "utils/utils.js", "content_scripts/block.js"],
      "matches": ["https://etherscan.io/block/*"]
    },
    {
      "js": ["utils/constants.js", "utils/utils.js", "content_scripts/account.js"],
      "matches": ["https://etherscan.io/address/*"]
    }
  ]
}
